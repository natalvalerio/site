{
  "manifest_version": 3,
  "name": "SONHAGRO",
  "version": "1.0",
  "description": "Links importantes para Franqueados Sonhagro!",
  "action": {
    "default_popup": "https://natalvalerio.github.io/sonhagro/sonhagro/popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "activeTab"
  ]
}
